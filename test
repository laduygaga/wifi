# user_add
curl -i -X POST -H "Content-Type: application/json" -d '{"user_id":1}' http://103.109.43.160:5002/v1/users

# user_del
curl -i -X DELETE http://103.109.43.160:5002/v1/users/1

# router_add
curl -i -X POST -H "Content-Type: application/json" -d '{"brand_short_name":"meraki"}' http://103.109.43.160:5002/v1/users/1/routers

# router_del
curl -i -X DELETE http://103.109.43.160:5002/v1/routers/5fdad0d233199a529db04283

# routers_list
curl  http://103.109.43.160:5002/v1/users/1/routers

# config
curl -i -X POST -H "Content-Type: application/json" -d '{"configuration_id":1, "value":"50afdb8906f5b437142bdbf3decc04fce7556b5c"}' http://103.109.43.160:5002/v1/routers/id1231234/configurations

## Meraki
# list_orgs
curl -X GET \
  -H "Content-type: application/json" \
  -H "Accept: application/json" \
  -d '{"api_key":"50afdb8906f5b437142bdbf3decc04fce7556b5c"}' \
  "http://103.109.43.160:5002/v1/meraki/organizations"

# list_networks
curl -X GET \
  -H "Content-type: application/json" \
  -H "Accept: application/json" \
  -d '{"api_key":"50afdb8906f5b437142bdbf3decc04fce7556b5c", "org_id":"111353"}' \
  "http://103.109.43.160:5002/v1/meraki/networks"

# list_ssids 
curl -X GET \
  -H "Content-type: application/json" \
  -H "Accept: application/json" \
  -d '{"api_key":"50afdb8906f5b437142bdbf3decc04fce7556b5c", "network_id":"N_572520102629493826"}' \
  "http://103.109.43.160:5002/v1/meraki/ssids"



# get_limit_bandwidth
curl -X GET \
  -H "Content-type: application/json" \
  -H "Accept: application/json" \
  -d '{"api_key":"50afdb8906f5b437142bdbf3decc04fce7556b5c", "network_id":"N_572520102629493826", "ssid_number":"0"}' \
  "http://103.109.43.160:5002/v1/meraki/limits"
# get_splash_timeout
curl -X GET \
  -H "Content-type: application/json" \
  -H "Accept: application/json" \
  -d '{"api_key":"50afdb8906f5b437142bdbf3decc04fce7556b5c", "network_id":"N_572520102629493826", "ssid_number":"0"}' \
  "http://103.109.43.160:5002/v1/meraki/splash_timeout"

# api_key_del
curl -i -X DELETE http://103.109.43.160:5002/v1/routers/5fdaddd8d6e4abcedf920267/api_key/del


## Unifi

# list sites
curl -X GET  \
-H "Content-type: application/json" \
-H "Accept: application/json" \
-d '{"host":"103.109.43.160", "port":8443, "username":"lazyhaha", "password":"nnd020895"}' \
"http://103.109.43.160:5002/v1/unifi/sites"

##  config sites
curl -i -X POST -H "Content-Type: application/json" -d '{"configuration_id":8, "value": {"host":"103.109.43.160", "port":8443, "username":"lazyhaha", "password":"nnd020895"}}' http://103.109.43.160:5002/v1/routers/5fdefc8261b184d4e763bfaf/configurations

# config portal serer
